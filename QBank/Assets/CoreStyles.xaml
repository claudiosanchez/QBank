                                                <Rectangle x:Name="DisabledVisualElement" Fill="{StaticResource DisabledBackgroundBrush}" IsHitTestVisible="false" Opacity="0" RadiusY="3" RadiusX="3" Stroke="{StaticResource DisabledBackgroundBrush}"/>
                                                <Grid Margin="3">
